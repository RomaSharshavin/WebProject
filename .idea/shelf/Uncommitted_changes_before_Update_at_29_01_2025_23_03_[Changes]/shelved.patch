n            transform: translateY(20px);\r\n        }\r\n        100% {\r\n            opacity: 1;\r\n            transform: translateY(0);\r\n        }\r\n    }\r\n\r\n    .partner:hover {\r\n        transform: scale(1.05);\r\n        box-shadow: 0 4px 20px rgba(0, 0, 0, 0.2); /* Тень при наведении */\r\n    }\r\n\r\n    .partner img {\r\n        width: 250px; /* Уменьшаем ширину изображения */\r\n        height: 150px; /* Высота авто для пропорций */\r\n        object-fit: contain;\r\n        border-radius: 2px;\r\n        margin-bottom: 5px;\r\n        margin-top: 5px /* Увеличиваем отступ снизу */\r\n    }\r\n\r\n    .partner a {\r\n        margin-top: 10px;\r\n        font-size: 18px;\r\n        font-weight: 500;\r\n        transition: color 0.3s;\r\n        text-decoration: none;\r\n        color: #0056b3; /* Цвет ссылок */\r\n    }\r\n\r\n    .partner a:hover {\r\n        color: #003d7a; /* Темнее при наведении */\r\n        text-decoration: underline; /* Подчеркивание при наведении */\r\n    }\r\n\r\n\r\n\r\n    /* Кнопка подробнее */\r\n    .button {\r\n        display: block; /* Делает ссылку блочным элементом */\r\n        width: 278px;\r\n        padding: 10px; /* Отступ внутри кнопки */\r\n        background-color: #a9def5; /* Цвет фона кнопки */\r\n        color: black; /* Цвет текста */\r\n        text-align: center; /* Центрирование текста */\r\n        text-decoration: none; /* Убираем подчеркивание */\r\n        border-radius: 5px; /* Скругление углов */\r\n        position: absolute; /* Абсолютное позиционирование */\r\n        bottom: 0px; /* Отступ от нижней границы контейнера */\r\n        left: 50%; /* Центрируем кнопку по горизонтали */\r\n        transform: translateX(-50%); /* Сдвигаем кнопку назад на половину её ширины */\r\n        transition: background-color 0.3s; /* Плавный переход цвета фона */\r\n    }\r\n    .button:hover {\r\n        background-color: #c7c7c7; /* Цвет фона при наведении */\r\n    }\r\n\r\n\r\n\r\n    /* настройка 